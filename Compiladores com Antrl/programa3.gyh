#Erro sintático: 'Inesperado Token: Var' ou 'Inesperado Lexema: parametro' ou 'Esperado Token PCDec' ou 'Esperado Lexema DEC'

:
parametro:INT
fatorial:INT

:PROG 
# Calcula o fatorial de um número inteiro
LER parametro
fatorial := parametro 
SE parametro == 0 ENTAO fatorial := 1
ENQTO parametro > 1
 INI
  fatorial := fatorial * (parametro - 1) 
  parametro := parametro - 1
 FIM
IMPRIMIR fatorial
IMPRIMIR "Oi"
